<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ejiaoyi.common.mapper.RegMapper">

    <!-- 通用查询映射结果 -->    <resultMap id="BaseResultMap" type="com.ejiaoyi.common.entity.Reg">
        <id column="ID" property="id"/>
        <result column="INSERT_TIME" property="insertTime"/>
        <result column="PARENT_ID" property="parentId"/>
        <result column="REG_NAME" property="regName"/>
        <result column="ORDER_NO" property="orderNo"/>
        <result column="ALL_SPELLING" property="allSpelling"/>
        <result column="FIRST_SPELLING" property="firstSpelling"/>
        <result column="REG_NO" property="regNo"/>
        <result column="ENABLED" property="enabled"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, INSERT_TIME, PARENT_ID, REG_NAME, ORDER_NO, ALL_SPELLING, FIRST_SPELLING, REG_NO, ENABLED
    </sql>

</mapper>
