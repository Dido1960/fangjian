<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ejiaoyi.expert.mapper.UploadFileMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ejiaoyi.common.entity.UploadFile">
        <id column="ID" property="id"/>
        <result column="INSERT_TIME" property="insertTime"/>
        <result column="FILE_UID" property="fileUid"/>
        <result column="NAME" property="name"/>
        <result column="SUFFIX" property="suffix"/>
        <result column="PATH" property="path"/>
        <result column="BYTE_SIZE" property="byteSize"/>
        <result column="READ_SIZE" property="readSize"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, INSERT_TIME, FILE_UID, NAME, SUFFIX, PATH, BYTE_SIZE, READ_SIZE
    </sql>

</mapper>
